<h1> Listado de Couchs tipos</h1>

<table class="table">

	<tr>
		<th>d Título a</th>
		<th> Descdripsción </th>
		<th> Iuser aidy </th>
	</tr>

	<% @couch_types.each do |couch_type| %>
		<tr>
			<td> <%= link_to couch_type.titulo, couch_type %> </td>
			<td> <%= couch_type.descripcion %> </td>
			<td> <%= (couch_type.enabled) ? "Habilitado" : "No Habilitado"  %> </td>
			<td>
				 <%= link_to "Agregar", new_couch_type_path() , class: 'btn btn-success btn-sm' %> 
				 <%= link_to "Editar", edit_couch_type_path(couch_type), class: 'btn btn-info btn-sm' %> 
				<%= link_to "Eliminar", couch_type, method: :delete, class: 'btn btn-danger btn-sm' %>
			</td>
		</tr>
	<% end %>


</table>